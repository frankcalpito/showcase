{
  "name": "showcase",
  "description": "Ready made components for rapid prototyping",
  "version": "0.0.1",
  "author": "Franklin Calpito Jr",
  "license": "MIT",
  "repository": "fccalpitojr/showcase",
  "keywords": [
    "react",
    "next",
    "showcase",
    "snackbar"
  ],
  "private": false,
  "scripts": {
    "clean": "rimraf dist",
    "dev": "next dev",
    "build": "next build",
    "build:components": "tsc -p tsconfig.bundle.json",
    "build:scss": "sass components:dist",
    "watch:scss": "sass --watch components:dist",
    "build:release": "cross-env node ./scripts/buildRelease.js",
    "release-components": "np ./dist",
    "start": "next start",
    "lint": " eslint **/*.tsx --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cypress": "cypress open",
    "test": "cypress run --component",
    "test:e2e": "cypress -- --e2e"
  },
  "dependencies": {
    "chart.js": "^4.3.3",
    "next": "13.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-interactions": "^7.0.6",
    "@storybook/addon-links": "^7.0.6",
    "@storybook/addon-styling": "^1.3.6",
    "@storybook/blocks": "^7.0.6",
    "@storybook/nextjs": "^7.0.6",
    "@storybook/react": "^7.0.6",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/node": "18.15.12",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vercel/analytics": "^1.0.2",
    "autoprefixer": "^10.4.15",
    "cross-env": "^7.0.3",
    "cypress": "^12.17.4",
    "eslint": "^8.47.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.13",
    "postcss": "^8.4.28",
    "react-ga4": "^2.1.0",
    "rimraf": "^5.0.1",
    "sass": "^1.65.1",
    "storybook": "^7.0.6",
    "storybook-tailwind-dark-mode": "^1.0.22",
    "tailwindcss": "^3.3.3"
  },
  "engines": {
    "node": ">=16"
  }
}
